create table users(id int primary key auto_increment, user_name varchar(255), public_name varchar(255), email varchar(255), password varchar(255), phone varchar(23), address text, fix_address text, profile_img varchar(255), company varchar(255), bio text, birth_date datetime, country varchar(100), state varchar(100), language varchar(255), fav_music varchar(200), fav_movie varchar(255), website varchar(255), role_id tinyint(4), status int(6), created_at timestamp, updated_at datetime);

                